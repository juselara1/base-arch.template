{
	"project_name": "Base Project",
	"project_slut": "{{ cookiecutter.project_name.lower().replace(' ', '_') }}",
	"dependencies": {
		"array": ["git", "chezmoi", "openssh", "neovim", "tmux", "starship", "zoxide", "fzf"]
	},
	"docker_args": {
		"array": ["DOTFILES_REPO", "DOTFILES_BRANCH"]
	},
	"extra_docker": {
		"array": [
			"RUN chezmoi init --apply --depth=1 --branch=${DOTFILES_BRANCH} ${DOTFILES_REPO}"
		]
	}
}
